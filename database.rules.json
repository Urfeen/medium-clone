{
  "rules": {
    "articles": {
      ".read": "true",
      "$article": {
        "$owner": {
          ".write": "auth != null && auth.uid == $owner"
        }
      }
    },
    "users": {
      "$uid": {
        ".read": "true",
        ".write": "auth != null && auth.uid == $uid"
      }
    }
  }
}
